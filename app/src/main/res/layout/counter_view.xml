<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

	<com.example.rzhu.counter.CounterNumberView
		android:id="@+id/sixth_digit"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="4dp" />

	<com.example.rzhu.counter.CounterNumberView
		android:id="@+id/fifth_digit"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="4dp" />

	<com.example.rzhu.counter.CounterNumberView
		android:id="@+id/forth_digit"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="4dp" />

	<ImageView
		android:id="@+id/comma"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="bottom"
		android:layout_marginRight="4dp"
		android:src="@drawable/android_lander_comma" />

	<com.example.rzhu.counter.CounterNumberView
		android:id="@+id/third_digit"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="4dp" />

	<com.example.rzhu.counter.CounterNumberView
		android:id="@+id/second_digit"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="4dp" />

	<com.example.rzhu.counter.CounterNumberView
		android:id="@+id/first_digit"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />


</merge>